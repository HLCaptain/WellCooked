<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_recipe_info"
    app:startDestination="@id/recipeInfoFragment">

    <fragment
        android:id="@+id/recipeInfoFragment"
        android:name="hu.wellcooked.fragment.customer.RecipeInfoFragment"
        android:label="RecipeInfoCourierFragment">
        <argument
            app:nullable="true"
            android:name="recipe"
            app:argType="Results"
            android:defaultValue="@null"/>
        <argument
            android:name="recipeId"
            app:argType="integer"
            android:defaultValue="-1"/>
    </fragment>
</navigation>