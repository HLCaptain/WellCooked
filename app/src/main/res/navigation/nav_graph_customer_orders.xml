<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph_customer_orders"
    app:startDestination="@id/customerOrdersFragment">
    <fragment
        android:id="@+id/customerOrdersFragment"
        android:name="hu.wellcooked.fragment.customer.CustomerOrdersFragment"
        android:label="CustomerOrdersFragment">
        <action
            android:id="@+id/action_customerOrdersFragment2_to_customerOrderInfoFragment"
            app:destination="@id/customerOrderInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/customerOrderInfoFragment"
        android:name="hu.wellcooked.fragment.customer.CustomerOrderInfoFragment"
        android:label="CustomerOrderInfoFragment">
        <action
            android:id="@+id/action_customerOrderInfoFragment_to_nav_graph_recipe_info"
            app:destination="@id/nav_graph_recipe_info" />
    </fragment>
    <include app:graph="@navigation/nav_graph_recipe_info" />
</navigation>